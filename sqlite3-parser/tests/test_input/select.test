%%

SELECT (1 + 1) + 2 / 4 + 3;

SelectStmt {
    "SELECT"
    ResultColumnList {
        ResultColumn {
            Expr {
                ExprInfix {
                    Expr {
                        ExprInfix {
                            Expr {
                                ExprParen {
                                    "("
                                    Expr {
                                        ExprInfix {
                                            Expr {
                                                ExprLit {
                                                    "1"
                                                }
                                            }
                                            OpAdd {
                                                "+"
                                            }
                                            Expr {
                                                ExprLit {
                                                    "1"
                                                }
                                            }
                                        }
                                    }
                                    ")"
                                }
                            }
                            OpAdd {
                                "+"
                            }
                            Expr {
                                ExprInfix {
                                    Expr {
                                        ExprLit {
                                            "2"
                                        }
                                    }
                                    OpDivide {
                                        "/"
                                    }
                                    Expr {
                                        ExprLit {
                                            "4"
                                        }
                                    }
                                }
                            }
                        }
                    }
                    OpAdd {
                        "+"
                    }
                    Expr {
                        ExprLit {
                            "3"
                        }
                    }
                }
            }
        }
    }
    ";"
}
