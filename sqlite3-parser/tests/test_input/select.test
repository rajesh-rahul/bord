%%

SELECT 1 BETWEEN 1 BETWEEN (6 + 9) + 3 AND 0 + 3 AND 0;

SelectStmt {
    "SELECT"
    ResultColumnList {
        ResultColumn {
            Expr {
                ExprInfix {
                    Expr {
                        ExprLit {
                            "1"
                        }
                    }
                    OpBetweenAnd {
                        "BETWEEN"
                        Expr {
                            ExprInfix {
                                Expr {
                                    ExprLit {
                                        "1"
                                    }
                                }
                                OpBetweenAnd {
                                    "BETWEEN"
                                    Expr {
                                        ExprInfix {
                                            Expr {
                                                ExprParen {
                                                    "("
                                                    Expr {
                                                        ExprInfix {
                                                            Expr {
                                                                ExprLit {
                                                                    "6"
                                                                }
                                                            }
                                                            OpAdd {
                                                                "+"
                                                            }
                                                            Expr {
                                                                ExprLit {
                                                                    "9"
                                                                }
                                                            }
                                                        }
                                                    }
                                                    ")"
                                                }
                                            }
                                            OpAdd {
                                                "+"
                                            }
                                            Expr {
                                                ExprLit {
                                                    "3"
                                                }
                                            }
                                        }
                                    }
                                    "AND"
                                }
                                Expr {
                                    ExprInfix {
                                        Expr {
                                            ExprLit {
                                                "0"
                                            }
                                        }
                                        OpAdd {
                                            "+"
                                        }
                                        Expr {
                                            ExprLit {
                                                "3"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                        "AND"
                    }
                    Expr {
                        ExprLit {
                            "0"
                        }
                    }
                }
            }
        }
    }
    ";"
}
